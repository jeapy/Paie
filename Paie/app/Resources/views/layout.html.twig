<!DOCTYPE html>
<html>
  <head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>{% block title %}€ASYPAY{% endblock %}</title>
  {% stylesheets filter='cssrewrite'
  'bundles/jpfichepaie/css/bootstrap.min.css'
  'bundles/jpfichepaie/css/bootstrap-theme.min.css'
   'bundles/jpfichepaie/css/datetimepicker.css'
   'bundles/jpfichepaie/css/styles.css' %}  
    <link href='http://fonts.googleapis.com/css?family=Lato:300' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
  <link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
<link href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css" rel="stylesheet">

{% endstylesheets %}
    
  </head>
  <body>
    
     <!-- *** TOP ***
_________________________________________________________ -->
            <div id="top">
                <div class="container">
                    <div class="row">
                        <div class="col-xs-5 contact">
                            <p class="hidden-sm hidden-xs">Contact</p>
                            <p class="hidden-md hidden-lg"><a href="#" data-animate-hover="pulse"><i class="fa fa-phone"></i></a>  <a href="#" data-animate-hover="pulse"><i class="fa fa-envelope"></i></a>
                            </p>
                        </div>
                        <div class="col-xs-7">
                            <div class="social">
                                <a href="#" class="external facebook" data-animate-hover="pulse"><i class="fa fa-facebook"></i></a>
                                <a href="#" class="external gplus" data-animate-hover="pulse"><i class="fa fa-google-plus"></i></a>
                                <a href="#" class="external twitter" data-animate-hover="pulse"><i class="fa fa-twitter"></i></a>
                                <a href="#" class="email" data-animate-hover="pulse"><i class="fa fa-envelope"></i></a>
                            </div>

                            <div class="login">

                               {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
                {{ 'layout.logged_in_as'|trans({'%username%': app.user.username}, 'FOSUserBundle') }} |
     <a class="deco" href="{{ path('fos_user_security_logout') }}"><span class="text-uppercase">{{ 'layout.logout'|trans({}, 'FOSUserBundle') }}  <i class="fa fa-sign-out"></i></span> </a>

            {% else %}
     <a class="deco" href="{{ path('fos_user_security_login') }}"><span class="text-uppercase">{{ 'layout.login'|trans({}, 'FOSUserBundle') }} <i class="fa fa-sign-in"></i></span></a>
              
            {% endif %}


                            </div>

                        </div>
                    </div>
                </div>
            </div>

            <!-- *** TOP END *** -->
    <div class="header">

       <div class="container">
          <div class="row">
             
<div class="container-fluid">
    <!-- Brand and toggle get grouped for better mobile display -->
    <div class="navbar-header">
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a class="navbar-brand" href="index.html"><img alt="EASYPAY" src="{{ asset('bundles/jpfichepaie/img/logo.png') }}"></a>
    </div>

    <!-- Collect the nav links, forms, and other content for toggling -->
    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
         
      <ul class="nav navbar-nav navbar-right">
     
         <li><a href="{{path('jp_fiche_paie_home')}}" >ACCUEIL</a></li>
        <li><a href="{{path('jp_fiche_paie_fiche')}}">FICHE DE PAIE</a></li>
        <li><a href="{{path('jp_fiche_paie_offre')}}">OFFRES</a></li>
        <li><a href="{{path('jp_fiche_paie_compte')}}">COMPTE</a></li>  

    

       {% if is_granted("ROLE_ADMIN") %}      
          <li><a href="{{path("jp_admin_homepage")}}">ADMIN</a></li>
          {% else %}
     
      {% endif %}

     
         
      </ul>

    </div><!-- /.navbar-collapse -->
  </div><!-- /.container-fluid -->
          </div>
       </div>
  </div>

    <div class="page-content">
       <div class="row">

           {% block body %}

        {% endblock %}


         

         

        </div>
    </div>

    <footer>
         <div class="container">
         
            <div class="copy text-center">
             
                &copy; {{ 'now'|date('Y') }} <br>
      Réalisé par <a href="http://www.jpzougou.pro">{{ webmaster }}.</a> 
     
            </div>
            
         </div>
      </footer>


  {% javascripts filter='jsqueeze'
  'bundles/jpfichepaie/js/bootstrap.min.js' 
  'bundles/jpfichepaie/js/custom.js'
  'bundles/jpfichepaie/js/forms.js'
  'bundles/jpfichepaie/js/moment.min.js'
  'bundles/jpfichepaie/js/bootstrap-datetimepicker.js'
  'bundles/jpfichepaie/js/datepicker-fr.js'
   %}
     <script src="https://code.jquery.com/jquery-1.12.4.js"></script>
  <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
<!--   <script src="https://code.jquery.com/jquery.js"></script>
   <script src="https://code.jquery.com/ui/1.10.3/jquery-ui.js"></script> -->
   
  <script type="text/javascript" src="{{ asset_url }}"></script>

{% endjavascripts %}




  </body>
</html>